{
    "query": {
        "name": "{{locationName}}
    },
    "location": {
        "id": {{location.id}},
        "city":{{location.city}},
        "latitude": "{{location.latitude}}",
        "longitude":"{{location.longitude}}",
    },
    "measurements":[
        {% for measurement in measurements %}
        {
            "date:"{{measurement.date|date('Y-m-d')}}",
            "celsius": {{measurement.celsius}}
        }
        {% if not loop.last %}, {% endif %}
        {% endfor %}
    ]
}